{
	"$schema": "https://turborepo.org/schema.json",
	"baseBranch": "origin/main",
	"pipeline": {
		"clean": {
			"outputs": ["dist/", "lib/"]
		},
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/", "lib/"]
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"lint": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"ci": {
			"dependsOn": ["build", "lint", "test"]
		},
		"dev": {
			"cache": false
		}
	}
}
